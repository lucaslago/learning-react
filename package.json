{
  "name": "learning-react",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "6.0.0"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "start:prod": "export NODE_ENV=production && npm run build:prod && node ./src/main-server",
    "start:dev": "export NODE_ENV=development && node ./src/main-server",
    "build:dev": "export NODE_ENV=development && npm run clean && webpack",
    "build:prod": "export NODE_ENV=production && npm run clean && webpack",
    "test": "export NODE_ENV=test && mocha ./src/**/*-Spec.js --compilers js:babel-core/register --reporter nyan --recursive --check-leaks --full-trace",
    "test:watch": "export NODE_ENV=test && mocha ./src/**/*-Spec.js --compilers js:babel-core/register --reporter nyan --recursive --check-leaks --full-trace -w",
    "test-unit": "export NODE_ENV=test && karma start",
    "lint": "eslint ./src --ext .js --ext .jsx --cache"
  },
  "author": "Lucas Lago",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "webpack": "^1.13.0",
    "html-webpack-plugin": "^2.16.0",
    "webpack-validator": "^1.1.0",
    "webpack-merge": "^0.12.0",
    "webpack-build-notifier": "^0.1.8",
    "css-loader": "^0.23.1",
    "style-loader": "^0.13.1",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.3",
    "jquery": "^2.2.3",
    "marked": "^0.3.5"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-react-hmre": "^1.1.1",
    "chai-enzyme": "^0.4.2",
    "enzyme": "^2.3.0",
    "expect": "^1.20.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-nyan-reporter": "^0.2.4",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.2",
    "react-shallow-testutils": "^2.0.0",
    "sinon": "^1.17.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1"
  }
}
